<null>
<box title="Attributes">

<atrgs>
	<ul>
	<atrg><li id="liAtrg.{{atrg_id}}">
		<a href="#" class="ancExpandAtrg"
			id="ancExpandAtrg.{{atrg_id}}">{{atrg_name}}</a>

		<a href="#" class="ancEditAtrg" id="ancEditAtrg.{{atrg_id}}"><img
			src="{{tempath}}catalog/edit.png"
			title="Edit" alt="Edit" /></a>

		<a href="#" class="ancDeleteAtrg" id="ancDeleteAtrg.{{atrg_id}}"><img
			src="{{tempath}}catalog/delete.png"
			title="Delete" alt="Delete" /></a>

		<ul class="ulAtrg" id="ulAtrg.{{atrg_id}}"></ul>
	</li>
	</atrg>
	<li><a href="#" class="ancCreateAtrg">Create Attribute Group</a></li>
	</ul>
</atrgs>
</box>

<attribs>
	<attrib>
	<li id="liAtr.{{atr_id}}">

		<a href="#" class="ancExpandAtr"
			id="ancExpandAtr.{{atr_id}}">{{atr_name}}</a>

		<a href="#" class="ancEditAtr" id="ancEditAtr.{{atr_id}}"><img
			src="{{tempath}}catalog/edit.png"
			title="Edit" alt="Edit" /></a>

		<a href="#" class="ancDeleteAtr" id="ancDeleteAtr.{{atr_id}}"><img
			src="{{tempath}}catalog/delete.png"
			title="Delete" alt="Delete" /></a>
	</li>
	<li><a href="#" class="ancCreateAtr">Create Attribute</a></li>
	</attrib>

	<selatr>
		<li><a href="#" class="ancCreateOpt"
			id="ancCreateOpt.{{atr_id}}">Create Option</a></li>
	</selatr>
</attribs>

<ul class="ulAtr" id="ulAtr.{{atr_id}}">
<options>
	<option><li id="liOpt.{{opt_id}}">
		<span id="spnOpt.{{opt_id}}">{{opt_name}}</span>:
		<span id="spnFor.{{opt_id}}">{{opt_formula}}</span>

	<a href="#" class="ancEditOpt" id="ancEditOpt.{{opt_id}}"><img
		src="{{tempath}}catalog/edit.png"
		title="Edit" alt="Edit" /></a>

	<a href="#" class="ancDeleteOpt" id="ancDeleteOpt.{{opt_id}}"><img
		src="{{tempath}}catalog/delete.png"
		title="Delete" alt="Delete" /></a>
	</li></option>
</options>
</ul>

<script type="text/javascript" src="{{tempath}}attribute/js.js" />
</null>