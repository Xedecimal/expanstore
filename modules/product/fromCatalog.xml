<null>
<product>
<div class="product" id="divProd_{{prod_id}}">
<prodhead />
<span class="product_name"><a
	href="product/view/{{prod_id}}">{{prod_name}}</a></span>

<prodneck />

	<!-- Properties -->
	<prodprops>
	<div class="divProdProps">
		<prodprop>
			<div class="product-prop">
			<div class="product-field">{{field}}</div>
			<div class="product-value">{{value}}</div>
			</div>
		</prodprop>
	</div>
	</prodprops>
	<!-- /Properties -->

	<!-- Images -->
	<div class="photos">
	<prodimage>
		<a href="{{large}}" class="thumb"><img src="{{small}}" alt="thumbnail" title="Click to enlarge" /></a>
	</prodimage>
	</div>
	<!-- /Images -->

	<div class="product-description">
	{{prod_desc}}
	</div>

	<div class="divProdControls">
		<callback ds="_d" name="product.cb.knee" />
	</div>

	<prodfoot/>

</div>
</product>

<callback ds="_d" name="product.cb.listing.foot" />

</null>
